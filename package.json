{
  "name": "dynameh",
  "version": "4.0.0",
  "description": "DynamoDB on Node more easier",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf ./dist",
    "docs": "rimraf ./docs && typedoc src/ --out docs --mode modules --module commonjs --target es6 --excludePrivate --excludeNotExported --exclude '**/*.test.ts' --name Dynameh && touch ./docs/.nojekyll && gh-pages -d docs -t",
    "lint": "tslint --project tsconfig.json",
    "prepublishOnly": "npm run clean && npm run build && npm run lint && npm run test && npm run docs",
    "test": "mocha --recursive --throw-deprecation --require ts-node/register \"src/**/*.test.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Giftbit/dynameh.git"
  },
  "keywords": [
    "dynamodb"
  ],
  "contributors": [
    "Jeffery Grajkowski <pushplay@gmail.com> (https://github.com/pushplay/)",
    "Graham Baradoy <graham@giftbit.com> (https://github.com/Baradoy/)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Giftbit/dynameh/issues"
  },
  "homepage": "https://github.com/Giftbit/dynameh#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.2.5",
    "@types/mocha": "^5.2.7",
    "@types/node": "12.x",
    "aws-sdk": "^2.580.0",
    "chai": "^4.2.0",
    "gh-pages": "^2.1.1",
    "mocha": "^6.2.2",
    "rimraf": "^3.0.0",
    "touch": "^3.1.0",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typedoc": "^0.15.3",
    "typescript": "^3.7.2"
  },
  "peerDependencies": {
    "aws-sdk": "^2.431.0"
  }
}

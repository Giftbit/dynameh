{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "noImplicitAny": false,
    "sourceMap": false,
    "declaration": true,
    "outDir": "dist"
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "**/*.test.ts"
  ],
  "typedocOptions": {
    "entryPoints": ["./src"],
    "exclude": "**/*.test.ts",
    "excludeExternals": true,
    "excludeInternal": true,
    "excludePrivate": true,
    "name": "Dynameh",
    "out": "./docs"
  }
}
